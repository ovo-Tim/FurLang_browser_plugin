{
  "name": "FurLang browser plug-in",
  "description": "A browser plug-in works with FurLang server, can provide some useful functions for language learners.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build_panel": "npm --prefix ./floating-panel run build",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/color-convert": "^2.0.3",
    "@types/vue-color": "^2.4.8",
    "axios": "^1.7.2",
    "bootstrap": "5.3.0-alpha1",
    "color-convert": "^2.0.1",
    "mdb-vue-ui-kit": "^5.0.0",
    "vue": "^3.4.27",
    "vue-color": "^2.8.1"
  },
  "devDependencies": {
    "@vue/cli":"5.0.8",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-typescript": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@wxt-dev/module-vue": "^1.0.0",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.21",
    "wxt": "^0.18.6"
  }
}
